L lmath.asm
@PRT "Start of Math: A:"
@PRTHEXI APTR_HIGH
@PRTHEXI APTR
@PRT " B:"
@PRTHEXI BPTR_HIGH
@PRTHEXI BPTR
@PRT " "
@PUSH BPTR
@PUSH APTR
@PUSH CPTR
@CALL SUB32
@PRTHEXI CPTR_HIGH
@PRTHEXI CPTR
@PRTNL
@END
